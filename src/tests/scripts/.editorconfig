# Some tests handle invalid syntax deliberately; exclude relevant attributes.
# See also the `file-format` section in `.pre-commit-config.yaml`.

[parser/features/mixed_indentation_on_blank_lines.rz]
trim_trailing_whitespace = false

[parser/warnings/empty_file_newline.norun.rz]
insert_final_newline = false

[parser/warnings/empty_file_newline_comment.norun.rz]
insert_final_newline = false
